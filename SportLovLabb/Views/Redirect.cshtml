@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Redirect>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = null;

    if (Model.IsTemporaryRedirect)
    {
        Response.Redirect(Model.Link.Url);
    }
    else
    {
        Response.RedirectPermanent(Model.Link.Url);
    }
}